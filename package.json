  "repository": {
    "type": "git",
    "url": "git+https://github.com/wittbulter/cfunc.git"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "ansi-escapes": "^4.2.0",
    "chalk": "^2.4.2",
    "func": "^1.0.4",
    "ncc": "^0.3.6",
    "ora": "^3.4.0",
    "prompt-sync": "^4.1.7",
    "prompts": "^2.1.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-lambdas": "^1.1.0",
    "typescript": "^3.5.2",
    "git-clone": "^0.1.0"
  }
}